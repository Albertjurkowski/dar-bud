    <script type="text/javascript">
        function loginUBlock() {
            var fbButton = $('#fbButtonBl').html();
            $('#fbButtonBl').html('');
            $("#fbButtonBl").append(fbButton);
        }

        setTimeout(function(){loginUBlock()},100);
        window.onfocus = function () {
            var fbButton = $("#facebook_login");
            if (fbButton.is(':hidden')) {
                setTimeout(function () {
                    loginUBlock()
                }, 100)
            }
        }
