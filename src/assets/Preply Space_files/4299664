    <script type="text/template" id="timeslot_view_new">
    

<div class="p-calendar__head<% if (self.type === 'popup') { %> p-calendar__head--rf<% } %>">
    <div class="btn-group btn-group--calendar-header-arrows<% if (self.type === 'popup') { %> btn-group--calendar-header-arrows-rf<% } %>">
        <div class="btn btn--arrow-left btn--group js-prev" data-qa-id="calendar-prev-week-link"></div>
        <div class="btn btn--arrow-right btn--group js-next" data-qa-id="calendar-next-week-link"></div>
    </div>
    <h2 class="p-calendar__head-title">
        <span><%= weekStart.format("MMM") %>&nbsp;</span>
        <span><%= weekStart.format("D") %></span>
        <span>&nbsp;–⁠&nbsp;</span>
        <% if (weekStart.format("MMM") !== weekEnd.format("MMM")) { %>
            <span><%= weekEnd.format("MMM") %>&nbsp;</span>
        <% } %>
        <span><%= weekEnd.format("D") %>,&nbsp;</span>
        <span><%= weekStart.year() %></span>
    </h2>
    <% if (self.type !== 'popup') {
        if (waffle.switch_is_active('timezone_confirmation')) { %>
            <div class="timezone-wrap-tutor-info-cal">
                <div id="js-timezone-confirm-button-mount-cal"></div>
            </div>
        <% } else { %>
            <div class="p-calendar__head-buttons">
                <label class="select-timezone-wrap">
                    <select name="timezone" id="timezone" class="select select--inline select--timezone">
                        <% timezones.forEach(function(timezone){ %>
                            <option value="<%= timezone.get('tzname') %>" <% if (timezone.get('tzname') === model.get('active_tz')) { %> selected<% } %>><%= timezone.fullName() %></option>
                        <% }) %>
                    </select>
                </label>
            </div>
        <% } %>
    <% } %>
</div>
<div class="p-calendar__content-inner <% if (expAmToPm) { %> p-calendar__new-time-format <% } %>">
    
    <% for(var day = weekStart; day < weekEnd; day.add(1, 'day')) {
        var timeslotsArray = dateArray[day.day()];
    %>
        <div class="p-calendar__col <% if (expAmToPm) { %> p-calendar__new-time-format <% } %>">
            <div class="p-calendar__col-inner  <%= self.getClassForTimeSlot(day, timeslotsArray, model) %> ">
                <div class="p-calendar__col-inner-head">
                    <p class="p-calendar__weekday"><%= day.format("ddd") %></p>
                    <p class="p-calendar__day"><%= day.format("D") %></p>
                </div>
                <% if (timeslotsArray.length) { %>
                    <% if (isShowNameScheduleSlotsFlagActive) { %>
                        <div class="p-calendar__col-inner-body<% if (self.type === 'popup') { %> p-calendar__col-inner-body--rf<% } %>">
                            <% for(var j=0; j < timeslotsArray.length; j++){ %>
                                <%
                                    var time = timeslotsArray[j];
                                %>
                                <% if (time['occupied'] && !time['inPast']) { %>
                                    <% for(var k=0; k < studentsArray.length; k++){ %>
                                        <%
                                            var student = studentsArray[k]
                                        %>
                                        <% if (time.format("HH:mm") === student.timeStart && time.day() === student.day && student.lessonType === "ONLINE_LESSON" ) { %>
                                            <span class="p-calendar__time hint-wrap occupied-slot disabled"
                                                data-qa-group="calendar-booked-timeslot"
                                                data-href="#requestNewModal/<%= self.getAlignedEpoch(time.valueOf(), day) %>"
                                                data-time="<%= time.format('HH:mm') %>"
                                            >
                                                <span class="hint hint--top hint--auto hint--delay">This time is pre-scheduled</span>
                                                <span
                                                    class="occupied-slot-text js-booked-timeslot"
                                                >
                                                    Бронь
                                                </span>
                                            </span>
                                            <% } %>
                                        <% } %>
                                <% } else { %>
                                    <span class="p-calendar__time <% if (showHint) { %>hint-wrap<% } %> <% if (time['inPast']) { %> disabled<% } else { %> available js-timeslot<% } %>"
                                        data-qa-group="<% if (!time['inPast']) { %>calendar-available-timeslot-link<% }%>"
                                        data-href="#requestNewModal/<%= self.getAlignedEpoch(time.valueOf(), day) %>"
                                        data-time="<%= time.format('HH:mm') %>"
                                    >
                                        <% if (expAmToPm) { %>
                                            <%= getNewFormatTime(time) %>
                                        <% } else { %>
                                            <%= time.format("HH:mm") %>
                                        <% } %>
                                        <% if (showHint && !time['inPast']) { %>
                                            <span class="hint hint--top hint--auto hint--delay hint--autohide"><i class="icon icon--ib"></i>Забронировать доступное время</span>
                                        <% } %>
                                    </span>
                                <% } %>
                            <% } %>
                        </div>
                    <% } else { %>
                        <div class="p-calendar__col-inner-body<% if (self.type === 'popup') { %> p-calendar__col-inner-body--rf<% } %>">
                            <% for(var j=0; j < timeslotsArray.length; j++){ %>
                                <%
                                    var time = timeslotsArray[j];
                                %>
                                <span class="p-calendar__time <% if (showHint) { %>hint-wrap<% } %><% if (time['inPast'] || time['occupied']) { %> disabled<% } else { %> available js-timeslot<% } %>"
                                    data-qa-group="<% if (!time['inPast'] && !time['occupied']) { %>calendar-available-timeslot-link<% }%> <% if (time['occupied']) { %>calendar-booked-timeslot<% }%>"
                                    data-href="#requestNewModal/<%= self.getAlignedEpoch(time.valueOf(), day) %>"
                                    data-time="<%= time.format('HH:mm') %>"
                                >
                                    <% if (expAmToPm) { %>
                                        <%= getNewFormatTime(time) %>
                                    <% } else { %>
                                        <%= time.format("HH:mm") %>
                                    <% } %>
                                    <% if (showHint && !time['inPast']) { %>
                                        <span class="hint hint--top hint--auto hint--delay hint--autohide"><i class="icon icon--ib"></i>Забронировать доступное время</span>
                                    <% } %>
                                </span>
                            <% } %>
                        </div>
                    <% } %>
                <% } %>
            </div>
        </div>
    <% } %>
</div>

</script>
