    window.waffle.FLAGS['student_calendar_syncing'] = true;
    window.waffle.FLAGS['credit_card_subscription_api'] = true;
    window.waffle.FLAGS['credit_card_subscription_api'] = true;
    window.waffle.FLAGS['exp_enable_stripe'] = false;
</script>

    
    
    
