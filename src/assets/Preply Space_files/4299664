<div class="modal modal--request">
    <div class="modal__avatar">
        <div class="avatars-group">
            <div class="avatar avatar--modal-top" style="background-image: url('<%= tutor_avatar_url %>')"></div>
        </div>
    </div>
    <div class="modal__content">
