on_lesson_confirmed': false,'get_node_ssr_homepage_from_cache': false,'delete_sps_from_es_task': true,'enable_braze_recovery_hours_taught_last_30d': true,'migrate_rationale': false,'enable_braze_recovery_last_lesson': true,'enable_braze_recovery_last_lesson_confirmed': true,'enable_braze_recovery_client_aggregate': true,'enable_braze_recovery_sync_all': true,'disconnect_gcalendar_if_inval_creds': true,'block_charges_outside_normal_subscription_flow': true,'knowio_question_hide_questions': true,'enable_braze_attribute_last_lead_subject_alias': true,'google_calendar_subscriptions_tutor_availability': true,'enable_force_login_second_factor_feature': true,'disallow_trial_rescheduling_due_to_advance_notice': true,'node_ssr_use_fetch_cached': true,'no_subscription_recovery_for_unsubscribed': true,'french_online_search_headline_keywords': false,'fetch_top_tutor_country_scores': true,'enable_braze_recovery_last_lead_subject_alias_task': true,'enable_braze_recovery_tutor': true,'enable_braze_recovery_send_user_profile': true,'enable_braze_recovery_send_employee': true,'enable_braze_recovery_tutor_multi_lang': true,'enable_braze_recovery_tutor_virtual_status': true };

    // not an arrow function due to backward compatibility
    const triggerHotjar = function (flagName) {
        window.hj = window.hj||function(){(hj.q=hj.q||[]).push(arguments)};
        window.hj('trigger', flagName);
        window.hj('tagRecording', [flagName]);
    }

    const trackExperiment = (flagName) => {
        const url = `/api/log_flag/${flagName}`;
        if (window.navigator && window.navigator.sendBeacon) {
            navigator.sendBeacon(url);
        } else {
            fetch(url, { credentials: 'include' })
        }
    }

    class FlagManager {
        constructor(getFlagIsActive) {
            this._requestsCache = {};
            this._flagsCache = {};
            this._getFlagIsActive = getFlagIsActive;
        }

        fetchFlagIsActive(flagName) {
            const cachedRequest = this._fetchFromCache(flagName);
            if (cachedRequest) {
                return cachedRequest;
            }

            const fetchRequest = this._getFlagIsActive(flagName)
                .then((isActive) => {
                    this._storeServerResponse(flagName, isActive)
                    return isActive;
                })
                .catch(() => {
                    this._deleteCachedRequest(flagName);
                    return false;
                })

            this._cacheServerRequest(flagName, fetchRequest);
            return fetchRequest;
        }

        _fetchFromCache(flagName) {
            if (this._flagsCache.hasOwnProperty(flagName)) {
                return Promise.resolve(!!this._flagsCache[flagName]);
            }

            var cachedIncompleteRequests = this._requestsCache[flagName];
            if (cachedIncompleteRequests) {
                return cachedIncompleteRequests
            }
        }

        _deleteCachedRequest(flagName) {
            delete this._requestsCache[flagName];
        }

        _storeServerResponse(flagName, isActive) {
            this._flagsCache[flagName] = isActive;
            this._deleteCachedRequest(flagName);
        }

        _cacheServerRequest(flagName, request) {
            this._requestsCache[flagName] = request;
        }
    }

    const trackingFlagManager = new FlagManager((flagName) => {
        return fetch('/api/flag_is_active/' + flagName, {credentials: 'same-origin'})
            .then(res => res.json())
            .then(data => {
                if (data.is_active_experiment && data.is_active) {
                    waffle.triggerHotjar(flagName);
                }
                return data.is_active
        })
    });

    const nonTrackingFlagManager = new FlagManager((flagName) => {
        return fetch('/graphql/', {
                method: 'POST',
                credentials: 'include',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    query: `query FetchNonTrackingFlag ($id: String!) {
                        flagIsActive(id: $id) {
                            isActive
                        }
                    }`,
                    variables: {
                        id: flagName
                    }
                })
            })
            .then(res => res.json())
            .then(res => res.data.flagIsActive.isActive)
    })

    window.waffle = {
        triggerHotjar,
        experimentsLog,
        trackExperiment,

        // we need to instatiate different flag requesters not only
        // because they fetch flags in a different way but also because
        // we can't use non-tracking flags cache for tracking one and vice versa.

        // we can't use one cache for tracking & non-tracking flag requesters because
        // if a tracking flag requester will fetch flag from non-tracking cache then
        // there's a possisbility that the user will experience the experiment but the flag
        // would not be tracked.
        trackingFlagManager,
        nonTrackingFlagManager,
        get FLAGS() {
            // left for backward compatibility
            return this.trackingFlagManager._flagsCache;
        },
        SWITCHES: SWITCHES,
        // Deprecated. Use `fetchFlagIsActive` instead.
        flag_is_active: function (flag_name) {
            return !!this.FLAGS[flag_name];
        },

        fetchFlagIsActiveNonTracking(flagName) {
            return this.nonTrackingFlagManager.fetchFlagIsActive(flagName);
        },
        fetchFlagIsActive: function (flagName) {
            return this.trackingFlagManager.fetchFlagIsActive(flagName);
        },
        switch_is_active: function waffle_switch(switch_name) {
            return !!SWITCHES[switch_name];
        },
    };
})();

    </script>
    
