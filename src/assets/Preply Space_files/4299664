<script>
    $(document).ready(function (){
        $('.js-soc-custom-button').each(function () {
            var $this = $(this),
                path = $this.data('pfb-path'),
                prevParam = $this.data('pfb-path-param'),
                queryParams = [];

            // TODO(asherepa) we can extend it in feature and parse all data attributes
            // using some pattern(param_name:value|another_param:another_value)
            if (typeof prevParam !== 'undefined') {
                queryParams.push(prevParam.split(':'));
            }

            if ($this.data('pfb-autodetect-tz')) {
                queryParams.push(['timezone', moment.tz.guess()]);
            }

            queryParams.push(['language', 'ru']);

            var clientRegType = localStorage.getItem('clientRegType');
            if (clientRegType) {
                queryParams.push(['registration_type', clientRegType]);
            }

            $this.attr('href', path + (queryParams.length ? '?' : '') + queryParams.map(function (pair) {
                return pair.join('=');
            }).join('&'));
        });
    });
</script>

    



<script>
(function (globals) {

  var django = globals.django || (globals.django = {});


  django.pluralidx = function (n) {
    var v=(n==1 ? 0 : n%10>=2 && n%10<=4 && (n%100<10 || n%100>=20) ? 1 : 2);
    if (typeof(v) == 'boolean') {
      return v ? 1 : 0;
    } else {
      return v;
    }
  };

